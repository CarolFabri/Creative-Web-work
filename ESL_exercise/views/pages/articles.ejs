    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Articles</title>
</head>
<body>
    <ul>
    <% for(const article of articles) { %>
        <li>
            <h2><%= article.title %></h2>
            <p><%= article.body %></p>
        </li>
        <hr />
    <% } %>
    </ul>
</body>
</html>
